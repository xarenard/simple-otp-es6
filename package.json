{
  "name": "@xa/simpleotp",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-proposal-export-default-from": "^7.5.2"
  },
  "scripts": {
    "start": "node ./bin/www",
    "lint": "eslint lib/**/**/*.js",
    "test": "./node_modules/.bin/mocha --require @babel/register  test/hotp/index.js",
    "coverage": "nyc mocha test/*/*.js --timeout=3000",
    "doc": "./node_modules/.bin/jsdoc -c jsdoc.json -d docs -e utf8",
    "build": "babel lib --out-dir dist",
    "build-example": "babel ./examples --out-dir dist/examples",
    "run-example": "babel-node ./examples/simpletotp.js",
    "clean": "rimraf dist",
    "clean-doc": "rimraf docs",
    "clean-all": "npm run clean && npm run clean-doc",
    "dist": "npm run clean && npm run build && npm test && npm run lint && npm run build & cp package.json dist/ && cp README.md dist/",
    "audit": "./node_modules/.bin/audit-ci --config audit-ci.json "

  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/node": "^7.5.0",
    "@babel/preset-env": "^7.5.4",
    "@babel/register": "^7.4.4",
    "audit-ci": "^2.1.0",
    "babel-eslint": "^10.0.2",
    "chai": "^4.2.0",
    "eslint": "^6.0.1",
    "jsdoc": "^3.6.2",
    "mocha": "^6.1.4",
    "nodemon": "^1.19.1",
    "rimraf": "^2.6.3"
  }
}
